# Unigram
*1:%x[-2,0]
*2:%x[-1,0]
*3:%x[ 0,0]
*4:%x[ 1,0]
*5:%x[ 2,0]

# Bigram
*6:%x[-1,0]/%x[0,0]
*7:%x[ 1,0]/%x[0,0]

# Trigram
*8:%x[-1,0]/%x[0,0]/%x[1,0]

# Fourgram
*9:%x[-2,0]/%x[-1,0]/%x[0,0]/%x[1,0]
*10:%x[-1,0]/%x[0,0]/%x[1,0]/%x[2,0]

# Fivegram
*11:%x[-2,0]/%x[-1,0]/%x[0,0]/%x[1,0]/%x[2,0]

# Sixgram
*12:%x[-3,0]/%x[-2,0]/%x[-1,0]/%x[0,0]/%x[1,0]/%x[2,0]
*13:%x[-2,0]/%x[-1,0]/%x[0,0]/%x[1,0]/%x[2,0]/%x[3,0]

# Sevengram
*14::%x[-3,0]/%x[-2,0]/%x[-1,0]/%x[0,0]/%x[1,0]/%x[2,0]/%x[3,0]
